{
  "expo": {
    "name": "NativeFy",
    "slug": "firstApp",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/edited_icon.png",
    "splash": {
      "image": "./assets/sqicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":"nativefy.com_download",
      "adaptiveIcon": {
        "foregroundImage": "./assets/criicon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
